{
  "name": "utl-app-juegos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run vite:build && npm run vite:preview",
    "dev": "npm run vite:dev",
    "container:start": "npm run vite:build && npm run vite:preview-container",
    "container:dev": "npm run vite:dev-container",
    "vite:dev-container": "vite --port $PORT --host",
    "vite:dev": "vite --host",
    "vite:preview": "vite preview --host",
    "vite:preview-container": "vite preview --port $PORT --host",
    "vite:build": "vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "vite": "^3.1.0"
  }
}
